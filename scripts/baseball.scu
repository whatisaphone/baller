script CastPlaySound@143 arg0 {
    if (arg0 < 98000) {
        if (not (sound-running 10002)) {
            sound.start arg0
            sound.channel global51
            sound.end
        }
    } else {
    }
}

script CastGameplay@144 dialogue arg1 arg2 arg3 arg4 arg5 arg6 arg7 arg8 arg9 {
    if gNoCommentary {
        stop-script 0
    }
    if (actor-get-room 1 != gCurrentRoom) {
        actor.init 1
        actor.new
        actor.costume 0
        put-actor 1 0 0 gCurrentRoom
    }
    actor.init 1
    actor.text-offset 0 250
    actor.talk-color 245
    if (dialogue <= DIALOGUE-WILD-PITCH) {
        start-script gDialogueHandlerScript dialogue arg1 arg2 arg3 arg4 arg5 arg6 arg7 arg8 arg9
        stop-script 0
    }
    case dialogue {
        DIALOGUE-HIT-FOUL {
            start-script CastHitFoul arg1
        }
        DIALOGUE-PITCH-BALL {
            start-script CastPitchBall arg1 arg2 arg3 arg4 arg5
        }
        DIALOGUE-FIELDER-RUNNING-TO-CATCH-FLY {
            start-script CastFielderRunningToCatchFly arg1
        }
        DIALOGUE-FIELDER-ABOUT-TO-CATCH {
            start-script CastFielderAboutToCatch arg1
        }
        DIALOGUE-FIELDER-DIVE {
            start-script CastFielderDive arg1
        }
        DIALOGUE-FIELDER-UNDER-POP-FLY {
            start-script CastFielderUnderPopFly arg1
        }
        DIALOGUE-HIT-FOUL-OUTFIELD {
            start-script CastHitFoulOutfield arg1
        }
        DIALOGUE-THROW {
            start-script CastThrow arg1
        }
        DIALOGUE-THROW-TO-FIRST {
            start-script CastThrowInfield arg1 1
        }
        DIALOGUE-THROW-TO-SECOND {
            start-script CastThrowInfield arg1 2
        }
        DIALOGUE-THROW-TO-THIRD {
            start-script CastThrowInfield arg1 3
        }
        DIALOGUE-THROW-TO-HOME {
            start-script CastThrowInfield arg1 0
        }

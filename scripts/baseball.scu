    var local1 local2 local3

    assign-string local2 "                              "
    while (gCustomKidNames[arg0][local1] != 0) {
        local2[local1] = gCustomKidNames[arg0][local1]
        inc local1
    }
    local2[local1] = gCustomKidNames[arg0][local1]
    sprintf local3 "%s" local2
    undim local2
    return local3
}


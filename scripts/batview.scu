local-script SendBatViewMessage@2049 message arg1 arg2 arg3 arg4 arg5 arg6 arg7 arg8 arg9 {
    if (message >= 7601 && gNetplayActive) {
        if (message >= MSG-RUN-PITCH) {
            start-script.rec maybe_net_send_message 40 2 NET-BATVIEW-MESSAGE message arg1 arg2 arg3 arg4 arg5 arg6 arg7 arg8 arg9
        } else if (in message [MSG-BATTER-CONTACT MSG-RESET-BATTER]) {
            start-script maybe_net_send_batview_message message arg1 arg2 arg3 arg4 arg5 arg6 arg7
            start-script.rec HandleBatViewMessage message 0 arg1 arg2 arg3 arg4 arg5 arg6 arg7 arg8 arg9
        } else if (message >= 7800) {
            start-script.rec maybe_net_send_message 20 1 NET-BATVIEW-MESSAGE message arg1 arg2 arg3 arg4 arg5 arg6 arg7 arg8 arg9
        } else {
            start-script.rec maybe_net_send_message 40 1 NET-BATVIEW-MESSAGE message arg1 arg2 arg3 arg4 arg5 arg6 arg7 arg8 arg9
        }
    } else {
        start-script.rec HandleBatViewMessage message 0 arg1 arg2 arg3 arg4 arg5 arg6 arg7 arg8 arg9
    }
}


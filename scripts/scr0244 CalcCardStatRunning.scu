; SCRP 244 CalcCardStatRunning

parameter kid: Kid
parameter arg1

local variable local2
local variable local3
local variable data: [Kid][KidData]

if (!arg1) {
    data = gBackupKidData
} else {
    data = gKidData
}
local2 = data[kid][KD-SPEED]
if (100 == local2) {
    local2 = 99
}
local3 = local2 / 10 + 1
return local3
free-script
